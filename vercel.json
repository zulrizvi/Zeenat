{
  "version": 2,
  "builds": [
    {
      "src": "react-page-flip-main/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "^/$", "dest": "/react-page-flip-main/dist/index.html" },
    { "src": "^/(.*)$", "dest": "/react-page-flip-main/dist/$1" }
  ]
}
