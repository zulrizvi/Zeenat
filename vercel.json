{
	"buildCommand": "cd react-page-flip-main && yarn install --frozen-lockfile && yarn build",
	"outputDirectory": "react-page-flip-main/dist",
	"rewrites": [
		{ "source": "/", "destination": "/react-page-flip-main/dist/index.html" },
		{ "source": "/(.*)", "destination": "/react-page-flip-main/dist/$1" }
	]
}
